<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Scheduled Action for Data Cleanup -->
    <record id="ir_cron_cleanup_old_data" model="ir.cron">
        <field name="name">ERP Health: Cleanup Old Data</field>
        <field name="model_id" ref="model_erp_health_config"/>
        <field name="state">code</field>
        <field name="code">model.cleanup_old_data()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active" eval="True"/>
    </record>
</odoo>